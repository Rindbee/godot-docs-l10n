msgstr "''{file} --line {line} --column {col}''"

msgid "Atom"
msgstr "Adamh"

msgid "``{file}:{line}``"
msgstr "''{file}:{line}''"

msgid "JetBrains Rider"
msgstr "Marcach JetBrains"

